{
  "name": "vue2-example",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --mode dev",
    "dev:prod": "vite --mode prod",
    "dev:local": "vite --mode local",
    "build": "vue-tsc && vite build --mode prod",
    "preview": "vite preview",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "lint": "npx lint-staged",
    "lint:es": "eslint --ext .js,.ts,.vue --ignore-path .eslintignore --fix src/",
    "lint:es:check": "eslint --ext .js,.ts,.vue --ignore-path .eslintignore src/",
    "lint:style": "stylelint ./src/**/*.{vue,css,scss} --fix",
    "lint:style:check": "stylelint ./src/**/*.{vue,css,scss}"
  },
  "dependencies": {
    "@itshixun/qst-ui-system": "^0.1.0",
    "@vueuse/core": "^9.13.0",
    "element-ui": "^2.15.13",
    "pinia": "^2.1.3",
    "vue": "^2.7.14",
    "vue-router": "^3.6.5"
  },
  "devDependencies": {
    "@iconify-json/line-md": "^1.1.24",
    "@iconify-json/mdi": "^1.1.52",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/node": "^16.18.32",
    "@vitejs/plugin-legacy": "^4.0.3",
    "@vitejs/plugin-vue2": "^2.2.0",
    "@vitejs/plugin-vue2-jsx": "^1.1.0",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.41.0",
    "eslint-plugin-vue": "^9.13.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.23",
    "postcss-html": "^1.5.0",
    "postcss-scss": "^4.0.6",
    "prettier": "^2.8.8",
    "sass": "^1.62.1",
    "stylelint": "^15.6.2",
    "stylelint-config-css-modules": "^4.2.0",
    "stylelint-config-recess-order": "^4.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard-scss": "^7.0.1",
    "stylelint-scss": "^4.7.0",
    "terser": "^5.17.4",
    "typescript": "~4.7.4",
    "unocss": "^0.52.0",
    "vite": "^4.3.8",
    "vue-tsc": "^0.38.9"
  },
  "lint-staged": {
    "./**/*.{vue,js,ts,jsx,tsx}": "eslint --ignore-path .eslintignore --fix",
    "./**/*.{vue,js,ts,jsx,tsx,md,json,scss,css,html}": "prettier . --write",
    "./**/*.{scss,css,vue}": "stylelint --fix"
  }
}

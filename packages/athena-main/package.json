{
  "name": "athena-main",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "packageManager": "pnpm@7.18.1",
  "scripts": {
    "dev": "vite --mode dev",
    "dev:prod": "vite --mode prod",
    "dev:local": "vite --mode local",
    "build": "vue-tsc && vite build --mode prod",
    "preview": "vite preview",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "lint": "npx lint-staged",
    "lint:es": "eslint --ext .js,.ts,.vue --ignore-path .eslintignore --fix src/",
    "lint:es:check": "eslint --ext .js,.ts,.vue --ignore-path .eslintignore src/",
    "lint:style": "stylelint ./src/**/*.{vue,css,scss} --fix",
    "lint:style:check": "stylelint ./src/**/*.{vue,css,scss}"
  },
  "dependencies": {
    "@iconify/json": "^2.2.3",
    "@vueuse/core": "^9.10.0",
    "@vueuse/head": "^1.0.22",
    "axios": "^1.2.2",
    "color2k": "^2.0.1",
    "element-plus": "^2.2.28",
    "js-base64": "^3.7.5",
    "lodash-es": "^4.17.21",
    "mitt": "^3.0.0",
    "pinia": "^2.0.28",
    "pinia-plugin-persist": "^1.0.0",
    "qst-ui-system": "workspace:^",
    "vanilla-colorful": "^0.7.2",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6",
    "wujie": "^1.0.13",
    "wujie-vue3": "^1.0.13"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/test-utils": "^2.2.7",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-vue": "^9.8.0",
    "jsdom": "^20.0.3",
    "postcss": "^8.4.20",
    "postcss-html": "^1.5.0",
    "postcss-scss": "^4.0.6",
    "prettier": "^2.8.1",
    "sass": "^1.57.1",
    "stylelint": "^15.3.0",
    "stylelint-config-css-modules": "^4.2.0",
    "stylelint-config-recess-order": "^4.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard-scss": "^7.0.1",
    "stylelint-scss": "^4.3.0",
    "typescript": "^4.9.4",
    "unocss": "^0.48.0",
    "vite": "^4.0.4",
    "vue-eslint-parser": "^9.1.0",
    "vue-tsc": "^1.0.22"
  },
  "lint-staged": {
    "./**/*.{vue,js,ts,jsx,tsx,json}": "eslint --ignore-path .eslintignore --fix",
    "./**/*.{vue,js,ts,jsx,tsx,md,json,scss,css,html}": "prettier . --write",
    "./**/*.{scss,css,vue}": "stylelint --fix"
  }
}

{
  "name": "athena-home-banner",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "packageManager": "pnpm@7.18.1",
  "scripts": {
    "dev": "vite --mode dev",
    "dev:prod": "vite --mode prod",
    "dev:local": "vite --mode local",
    "build": "vue-tsc && vite build --mode prod",
    "preview": "vite preview",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "lint": "npx lint-staged",
    "lint:es": "eslint --ext .js,.ts,.vue --ignore-path .eslintignore --fix src/",
    "lint:es:check": "eslint --ext .js,.ts,.vue --ignore-path .eslintignore src/",
    "lint:style": "stylelint ./src/**/*.{vue,css,scss} --fix",
    "lint:style:check": "stylelint ./src/**/*.{vue,css,scss}"
  },
  "dependencies": {
    "@itshixun/qst-ui-system": "^0.1.0",
    "@vueuse/core": "^10.1.2",
    "@vueuse/head": "^1.1.26",
    "axios": "^1.4.0",
    "color2k": "^2.0.2",
    "element-plus": "^2.3.5",
    "lodash-es": "^4.17.21",
    "pinia": "^2.1.3",
    "pinia-plugin-persist": "^1.0.0",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@iconify-json/line-md": "^1.1.24",
    "@iconify-json/mdi": "^1.1.52",
    "@types/lodash-es": "^4.17.7",
    "@types/node": "^20.2.3",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "@vitejs/plugin-vue": "^4.2.3",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-vue": "^9.13.0",
    "jsdom": "^22.0.0",
    "postcss": "^8.4.23",
    "postcss-html": "^1.5.0",
    "postcss-scss": "^4.0.6",
    "prettier": "^2.8.8",
    "sass": "^1.62.1",
    "stylelint": "^15.6.2",
    "stylelint-config-css-modules": "^4.2.0",
    "stylelint-config-recess-order": "^4.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard-scss": "^9.0.0",
    "stylelint-scss": "^5.0.0",
    "typescript": "^5.0.4",
    "unocss": "^0.52.0",
    "vite": "^4.3.8",
    "vue-eslint-parser": "^9.3.0",
    "vue-tsc": "^1.6.5"
  },
  "lint-staged": {
    "./**/*.{vue,js,ts,jsx,tsx}": "eslint --ignore-path .eslintignore --fix",
    "./**/*.{vue,js,ts,jsx,tsx,md,json,scss,css,html}": "prettier . --write",
    "./**/*.{scss,css,vue}": "stylelint --fix"
  }
}

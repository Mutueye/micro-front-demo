{
  "name": "athena-home-banner",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "packageManager": "pnpm@7.18.1",
  "scripts": {
    "dev": "vite --mode dev",
    "dev:prod": "vite --mode prod",
    "dev:local": "vite --mode local",
    "build": "vue-tsc && vite build --mode prod",
    "preview": "vite preview",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "lint": "npx lint-staged",
    "lint:es": "eslint --ext .js,.ts,.vue --ignore-path .eslintignore --fix src/",
    "lint:es:check": "eslint --ext .js,.ts,.vue --ignore-path .eslintignore src/",
    "lint:style": "stylelint ./src/**/*.{vue,css,scss} --fix",
    "lint:style:check": "stylelint ./src/**/*.{vue,css,scss}"
  },
  "dependencies": {
    "@iconify/json": "^2.2.50",
    "@vueuse/core": "^9.13.0",
    "@vueuse/head": "^1.1.23",
    "axios": "^1.3.5",
    "color2k": "^2.0.2",
    "element-plus": "^2.3.3",
    "lodash-es": "^4.17.21",
    "pinia": "^2.0.34",
    "pinia-plugin-persist": "^1.0.0",
    "qst-ui-system": "workspace:^",
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.7",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "@vitejs/plugin-vue": "^4.1.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-vue": "^9.10.0",
    "jsdom": "^20.0.3",
    "postcss": "^8.4.21",
    "postcss-html": "^1.5.0",
    "postcss-scss": "^4.0.6",
    "prettier": "^2.8.7",
    "sass": "^1.62.0",
    "stylelint": "^15.4.0",
    "stylelint-config-css-modules": "^4.2.0",
    "stylelint-config-recess-order": "^4.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard-scss": "^7.0.1",
    "stylelint-scss": "^4.6.0",
    "typescript": "^4.9.5",
    "unocss": "^0.48.5",
    "vite": "^4.2.1",
    "vue-eslint-parser": "^9.1.1",
    "vue-tsc": "^1.2.0"
  },
  "lint-staged": {
    "./**/*.{vue,js,ts,jsx,tsx}": "eslint --ignore-path .eslintignore --fix",
    "./**/*.{vue,js,ts,jsx,tsx,md,json,scss,css,html}": "prettier . --write",
    "./**/*.{scss,css,vue}": "stylelint --fix"
  }
}
